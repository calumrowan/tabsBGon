{
  "manifest_version": 2,
  "name": "Close Domain's Tabs",
     "description": "Closes all tabs whose domain matches the active tab's",
  "short_name":"TabsBGon",
  "author": "Calum Rowan",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "static/icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "commands":{
	"_execute_browser_action":{
		"suggested_key":{
			"default": "Ctrl+Shift+X"
		}
	}
  },
  "icons": {
    "16": "static/icon-16.png",
    "32": "static/icon-32.png",
    "48": "static/icon-48.png",
    "64": "static/icon-64.png",
    "128": "static/icon-128.png"
  }
}
